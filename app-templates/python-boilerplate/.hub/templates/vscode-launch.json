{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "cloudcode",
      "language": "Python",
      "request": "launch",
      "imageRegistry": "CHANGEMEPLEASE",
      "name": "%s: Launch on Kubernetes",
      "skaffoldConfig": "${workspaceFolder}/skaffold.yaml",
      "profile": "local",
      "watch": true,
      "localRoot": "\"${workspaceFolder}/src\"",
      "remoteRoot": "/app",
      "podSelector": {
        "app": "CHANGEMEPLEASE"
      },
      "debug": [
        {
          "localRoot": "\"${workspaceFolder}/src\"",
          "remoteRoot": "/app"
        }
      ]
    },
    {
      "type": "cloudcode",
      "language": "Python",
      "request": "attach",
      "debugPort": 3000,
      "remoteRoot": "/app",
      "name": "%s: Attach to running container",
      "skaffoldConfig": "${workspaceFolder}/skaffold.yaml",
      "profile": "local",
      "watch": true,
      "localRoot": "\"${workspaceFolder}/src\"",
      "podSelector": {
        "app": "CHANGEMEPLEASE"
      },
      "debug": [
        {
          "localRoot": "\"${workspaceFolder}/src\"",
          "remoteRoot": "/app"
        }
      ]
    }
  ]
}
